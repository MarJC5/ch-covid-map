{
  "homepage": "https://marjc5.github.io/ch-covid-map/",
  "name": "ch-covid-map",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "13.3.0",
    "body-parser": "1.19.0",
    "concurrently": "5.3.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-pino-logger": "6.0.0",
    "gh-pages": "2.2.0",
    "http-proxy-middleware": "1.2.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "node-env-run": "3.0.2",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "object-path": "0.11.8",
    "pino-colada": "1.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "sass": "1.32.11",
    "tailwindcss": "2.2.16",
    "yargs-parser": "20.2.9"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "concurrently \"npm run server\" \"npm run client \"",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/stylesheet/tailwind.css -o src/assets/stylesheet/index.css",
    "watch:css": "postcss src/assets/stylesheet/tailwind.css -o src/assets/stylesheet/index.css",
    "client": "npm run watch:css && react-scripts start",
    "server": "node-env-run server --exec nodemon | pino-colada",
    "dev": "run-p server start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "9.8.8",
    "postcss-cli": "7.1.2"
  }
}
